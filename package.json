{
  "name": "data-center-intel",
  "version": "0.1.0",
  "private": true,
  "description": "Data Center Intel - Track U.S. data center developments",
  "scripts": {
    "dev": "pnpm -r --parallel run dev",
    "build": "pnpm -r run build",
    "test": "pnpm -r run test",
    "test:e2e": "pnpm -F tests run test:e2e",
    "lint": "pnpm -r run lint",
