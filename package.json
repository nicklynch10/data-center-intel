    "db:migrate": "pnpm -F db run migrate"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "dotenv": "^16.4.1",
    "dotenv-cli": "^7.3.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.1"
}